<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find gazebo_worlds)/worlds/diff_robot.world"/>
    <!-- more default parameters can be changed here -->
  </include>
  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" args="-file $(find diff_robot)/models/diff_robot_model.sdf -sdf -z 1 -model diff_robot" />
</launch>
